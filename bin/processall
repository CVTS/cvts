#!/usr/bin/env python

import logging
import luigi
from cvts.tasks import (
    RasterCounts,
    RegionCounts,
    SourceDestinationCounts,
    AverageSpeed,
    TripsToDB
    )

luigi.build([
    RasterCounts(),
    RegionCounts('District'),
    SourceDestinationCounts('trans_cities'),
    AverageSpeed(),
    TripsToDB()
    ], local_scheduler=True)
