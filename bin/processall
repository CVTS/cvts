#!/usr/bin/env python

import logging
import luigi
from cvts.tasks import (
    RasterCounts,
    RegionCounts,
    SourceDestinationCounts,
    AverageSpeed)

luigi.build([
    RasterCounts(),
    RegionCounts('District'),
    SourceDestinationCounts('trans_cities'),
    AverageSpeed()], local_scheduler=True)
