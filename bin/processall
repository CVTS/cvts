#!/usr/bin/env python

import luigi
from cvts.settings import setup_logging
from cvts.tasks import (
    RasterCounts,
    RegionCounts,
    SourceDestinationCounts,
    AverageSpeed,
    TripsToDB)

setup_logging()

luigi.build([
    RasterCounts(),
    RegionCounts('District'),
    SourceDestinationCounts('trans_cities'),
    AverageSpeed(),
    TripsToDB()
    ], local_scheduler=True)
